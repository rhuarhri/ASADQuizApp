<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rhuarhri.asadquizapp">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreateQuizActivity" />
        <activity android:name=".SetupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".LectureHomeActivity" />
        <activity android:name=".CreateQuestionActivity" />
        <activity android:name=".StudentHomeActivity" />
        <activity android:name=".AnswerQuizActivity" />
        <activity android:name=".studentAccessActivity" />
        <activity android:name=".LectureQuizManagerActivity" />
        <activity android:name=".SignUpActivity"></activity>
    </application>

</manifest>